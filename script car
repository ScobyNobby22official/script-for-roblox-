script:




script.Parent.Touched:Connect(function(part)
	if part:FindFirstChild("SignalValues") == nil then
		
	else
		if part:FindFirstChild("SignalValues").SignalType.Value == "TriggerOnly" then
			part:FindFirstChild("SignalValues").Triggered.Value = true
		end
		if part:FindFirstChild("SignalValues").SignalType.Value == "ActivateDelete" then
			script.Parent.Parent.Parent.Parent.TrainValues.ActivateDelete.Value = true
		end
	end
end) 

